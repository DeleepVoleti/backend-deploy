pipeline {
    agent {
        label 'Agent-1'
    }
    options {
        timeout(time: 30, unit: 'MINUTES')
        disableConcurrentBuilds()
        ansiColor('xterm')
    }
    stages {
        stage('read the version'){
            steps{
                script{
                    echo appversion is :${appVersion}
                }
            }
        }
   }
    post { 
        always { 
            deleteDir()
        }
    }
}